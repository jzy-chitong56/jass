{
  "name": "jass",
  "displayName": "jass",
  "description": "jass插件,QQ群:786204376",
  "publisher": "jass",
  "icon": "static/jass.png",
  "version": "1.8.29",
  "keywords": [
    "jass",
    "vjass",
    "zinc",
    "warcraft"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "markdown": "github",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naichabaobao/jass"
  },
  "homepage": "https://github.com/naichabaobao/jass",
  "activationEvents": [
    "onLanguage:jass"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jass",
      "properties": {
        "jass.lua.dependents": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "(Deprecation) '.lua' file library"
        },
        "jass.support.lua": {
          "type": "boolean",
          "default": false,
          "description": "(Deprecation) Support lua, Cooperate with 'jass.lua.dependents' consumption"
        },
        "jass.cjass.dependents": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "(Deprecation) cjass standard library"
        },
        "jass.support.cjass": {
          "type": "boolean",
          "default": false,
          "description": "(Deprecation) support cjass(not recommended)"
        },
        "jass.diagnostic": {
          "type": "boolean",
          "default": true,
          "description": "(Test) diagnostic support,does not support including Lua, Zinc, and pre-processing code"
        }
      }
    },
    "languages": [
      {
        "id": "jass",
        "aliases": [
          "jass",
          "jass2"
        ],
        "extensions": [
          ".j",
          ".ai",
          ".jass",
          ".zn"
        ],
        "configuration": "./src/jass-configuration.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "source.jass",
        "language": "jass",
        "path": "./src/jass.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jass",
        "path": "./src/snippets.json"
      }
    ],
    "documentFormattingProvider": true,
    "enableProposedApi": false,
    "codeLensProvider": {
      "resolveProvider": "true"
    },
    "menus": {
    }
  },
  "capabilities": {
    "documentSymbolProvider": "true",
    "workspaceSymbolProvider": "true"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc  -p ./",
    "compilep": "tsc  -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "prepublish": "vsce publish --pre-release --no-dependencies",
    "temp": "tsc && node ./out/boot/jass/tokens.js",
    "temp2": "tsc && node ./out/boot/jass/evaluator.js",
    "test:dev": "node ./out/boot/jass/parser-vjass.js"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.24.0",
    "typescript": "^4.9.4",
    "vsce": "^2.15.0"
  },
  "bugs": {
    "url": "https://github.com/naichabaobao/jass/issues"
  },
  "dependencies": {
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.49.3"
  },
  "author": "家猫hoo",
  "license": "GPL"
}
